{
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "api-gateway.js",
    "!node_modules/**",
    "!build/**",
    "!coverage/**"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "testMatch": [
    "**/__tests__/**/*.test.js",
    "**/?(*.)+(spec|test).js"
  ],
  "verbose": true,
  "collectCoverage": false,
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "setupFilesAfterEnv": [],
  "testTimeout": 10000
}